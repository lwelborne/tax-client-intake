{
  "name": "server",            // Project/package name (from package.json)
  "version": "1.0.0",          // Project version
  "lockfileVersion": 3,        // Lockfile format (npm v9 uses version 3)
  "requires": true,            // Indicates dependencies are required
  "packages": {
    "": {
      "name": "server",        // Root project again
      "version": "1.0.0",
      "license": "ISC",
      "dependencies": {        // Production dependencies (from package.json)
        "axios": "^1.12.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-validator": "^7.2.1",
        "mongodb": "^6.19.0",
        "mongoose": "^8.18.0"
      },
      "devDependencies": {     // Dev-only dependencies
        "nodemon": "^3.1.0"
      }
    },

    // Example: Express installed
    "node_modules/express": {
      "version": "4.21.2",      // Exact installed version (locked, not a range)
      "resolved": "https://registry.npmjs.org/express/-/express-4.21.2.tgz",
      "integrity": "sha512-Gd...==", // Hash for security (verifies download)
      "license": "MIT",         // License info
      "dependencies": {         // Sub-dependencies Express needs
        "accepts": "~1.3.8",
        "array-flatten": "1.1.1",
        "body-parser": "1.20.3",
        "debug": "2.6.9",
        "qs": "6.13.0",
        "send": "0.19.0"
      },
      "engines": {
        "node": ">= 0.10.0"     // Minimum Node.js version required
      }
    },

    // Example: Mongoose installed
    "node_modules/mongoose": {
      "version": "8.18.1",
      "resolved": "https://registry.npmjs.org/mongoose/-/mongoose-8.18.1.tgz",
      "integrity": "sha512-JK...==",
      "license": "MIT",
      "dependencies": {
        "mongodb": "~6.18.0"    // Notice: mongoose requires a specific MongoDB driver version
      },
      "engines": {
        "node": ">=16.20.1"     // Mongoose requires Node.js 16.20.1+
      }
    }
  }
}

